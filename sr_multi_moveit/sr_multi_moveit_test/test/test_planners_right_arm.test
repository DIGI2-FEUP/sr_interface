<launch>
  <include file = "$(find sr_multi_moveit_config)/launch/demo.launch">
        <arg name = "visualization" value = "false" />
        <arg name="robot_description" default="$(find sr_multi_description)/urdf/right_srhand_ur10_joint_limited.urdf.xacro"/>
        <arg name = "robot_config_file" value="$(find sr_multi_moveit_config)/config/robot_configs/right_sh_ur10.yaml" />
  </include>

  <test test-name = "test_planner_BKPIECEkConfigDefault" pkg = "sr_multi_moveit_test" type = "test_planners.py"
        time-limit = "300.0" retry = "3" args = "right_arm BKPIECEkConfigDefault" />
  <test test-name = "test_planner_ESTkConfigDefault" pkg = "sr_multi_moveit_test" type = "test_planners.py"
        time-limit = "300.0" retry = "3" args = "right_arm ESTkConfigDefault" />
  <test test-name = "test_planner_KPIECEkConfigDefault" pkg = "sr_multi_moveit_test" type = "test_planners.py"
        time-limit = "300.0" retry = "3" args = "right_arm KPIECEkConfigDefault" />
  <test test-name = "test_planner_LBKPIECEkConfigDefault" pkg = "sr_multi_moveit_test" type = "test_planners.py"
        time-limit = "300.0" retry = "3" args = "right_arm LBKPIECEkConfigDefault" />
  <test test-name = "test_planner_PRMkConfigDefault" pkg = "sr_multi_moveit_test" type = "test_planners.py"
        time-limit = "300.0" retry = "3" args = "right_arm PRMkConfigDefault" />
  <test test-name = "test_planner_RRTkConfigDefault" pkg = "sr_multi_moveit_test" type = "test_planners.py"
        time-limit = "300.0" retry = "3" args = "right_arm RRTkConfigDefault" />
  <test test-name = "test_planner_SBLkConfigDefault" pkg = "sr_multi_moveit_test" type = "test_planners.py"
        time-limit = "300.0" retry = "3" args = "right_arm SBLkConfigDefault" />
  <test test-name = "test_planner_PRMstarkConfigDefault" pkg = "sr_multi_moveit_test" type = "test_planners.py"
        time-limit = "300.0" retry = "3" args = "right_arm PRMstarkConfigDefault" />
  <test test-name = "test_planner_RRTstarkConfigDefault" pkg = "sr_multi_moveit_test" type = "test_planners.py"
        time-limit = "300.0" retry = "3" args = "right_arm RRTstarkConfigDefault" />

</launch>